<h2>Create a new drone</h2>
<form method="POST" action="/drones">
    <div>
        <label for="name">Name</label>
        <input type="text" id="name" name="name" />
    </div>
    <div>
        <labe for="propellers">Propellers</labe>
        <input type="text" id="propellers" name="propellers" />
    </div>
    <div>
        <label for="maxSpeed">Max Speed</label>
        <input type="text" id="maxSpeed" name="maxSpeed" />
    </div>
    <button>Submit !</button>
</form>
