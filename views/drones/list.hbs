<h2>Available drones</h2>
<ul>
    {{#each drones}} 
    <li> 
        {{ this.name}}
        {{ this.propellers}}
        {{ this.maxSpeed}}
        <a href="/drones/{{this.id}}/edit">Edit</a>
        <form method="POST" action="/drones/{{this.id}}/delete">
        <button>Delete</button>

        </form>

    </li>
    {{/each}}
</ul>
